<h1>Ticket <%= "#{@ticket.id}" %></h1>

<table>
  <thead>
  <tr>
    <th>User name</th>
    <th>User last name</th>
    <th>User passport</th>
    <th>Route</th>
  </tr>
  </thead>

  <tbody>
  <td><%= @ticket.first_name_user %></td>
  <td><%= @ticket.last_name_user %></td>
  <td><%= @ticket.passport_user %></td>
  <td><%= "from #{@ticket.base_station} to #{@ticket.end_station}" %></td>
  <td><%= link_to 'Delete', ticket_path(@ticket), method: :delete %></td>
  </tbody>
</table>

<br>