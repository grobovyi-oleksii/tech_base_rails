<h1>Tickets</h1>

<table>
  <thead>
  <tr>
    <th>Ticket id</th>
    <th>User name</th>
    <th>User last name</th>
    <th>User passport</th>
  </tr>
  </thead>

  <% @tickets.each do |ticket| %>
    <tbody>
    <td><%=link_to ticket.id, ticket_path(ticket) %></td>
    <td><%= ticket.first_name_user %></td>
    <td><%= ticket.last_name_user %></td>
    <td><%= ticket.passport_user %></td>

    </tbody>
  <% end %>
</table>

<br>

<%= link_to 'Search page', search_path %>