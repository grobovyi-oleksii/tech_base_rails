<h1>Tickets</h1>

<table>
  <thead>
  <tr>
    <th>Ticket id</th>
    <th>User name</th>
    <th>User last name</th>
    <th>User passport</th>
    <th>Route</th>
  </tr>
  </thead>


  <% @tickets.each do |ticket| %>
    <tbody>
    <td><%= link_to "#{ticket.id}", admin_ticket_path(ticket.id) %></td>
    <td><%= ticket.first_name_user %></td>
    <td><%= ticket.last_name_user %></td>
    <td><%= ticket.passport_user %></td>
    <td><%= "from #{ticket.base_station} to #{ticket.end_station}" %></td>
    </tbody>
  <% end %>

</table>

<br>